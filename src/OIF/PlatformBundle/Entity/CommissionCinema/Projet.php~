    /**
     * Set support
     *
     * @param string $support
     *
     * @return Projet
     */
    public function setSupport($support)
    {
        $this->support = $support;

        return $this;
    }

    /**
     * Get support
     *
     * @return string
     */
    public function getSupport()
    {
        return $this->support;
    }

    /**
     * Set standard
     *
     * @param string $standard
     *
     * @return Projet
